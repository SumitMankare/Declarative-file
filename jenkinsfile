pipeline {
    agent any

    stages {
        stage('pull') {
            steps {
                echo 'Pulled'
            }
        }
        stage('build') {
            steps {
                echo 'Build'
            }
        }
        stage('test') {
            steps {
                echo 'Tested'
            }
        }
        stage('deploy') {
            steps {
                echo 'Deployed'
            }
        }
    }
}
